usdt:/home/zixianc/mmtk-ebpf/lxr/openjdk/build/linux-x86_64-normal-server-release/jdk/lib/server/libmmtk_openjdk.so:mmtk:work_poll {
    $ns = nsecs;
    //printf("WORK_POLL,B,%d,%lu\n", tid, $ns);
    if (@thread_in_work[tid]) {
        if (@enable_print) {
            printf("WORK,E,%d,%lu,%lu\n", tid, $ns, @thread_work_id[tid]);
        }
    }
}

usdt:/home/zixianc/mmtk-ebpf/lxr/openjdk/build/linux-x86_64-normal-server-release/jdk/lib/server/libmmtk_openjdk.so:mmtk:work {
    $ns = nsecs;
    $work_name = str(arg0, arg1);
    @thread_in_work[tid] = 1;
    if (!(@decoded[arg0])) {
        @type_name[arg0] = str(arg0, arg1);
        @decoded[arg0] = 1;
    }
    @thread_work_id[tid] = arg0;
    //printf("WORK_POLL,E,%d,%lu\n", tid, $ns);
    if (@enable_print) {
        printf("WORK,B,%d,%lu,%lu\n", tid, $ns, arg0);
    }
}

